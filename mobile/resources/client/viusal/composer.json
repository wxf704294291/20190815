{
    "name": "dscmall/mobile",
    "description": "DSCMall E-Commerce System.",
    "keywords": ["dscmall", "ectouch", "ecshop", "ecmall", "wemall"],
    "homepage": "https://www.flyobd.com/",
    "license": "GPL",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "aliyuncs/oss-sdk-php": "^2.2",
        "dingo/api": "1.0.x@dev",
        "doctrine/dbal": "^2.5",
        "erusev/parsedown": "^1.6",
        "laravel/lumen-framework": "5.4.*",
        "mobiledetect/mobiledetectlib": "^2.8",
        "overtrue/easy-sms": "^0.0.4",
        "overtrue/pinyin": "~3.0",
        "phpoffice/phpexcel": "^1.8",
        "riverslei/payment": "~3.1",
        "sentry/sentry": "^1.6",
        "tymon/jwt-auth": "0.5.*",
        "vlucas/phpdotenv": "~2.2",
        "workerman/phpsocket.io": "^1.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
