{
    "name": "dscmall/dscmall",
    "description": "DSCMall E-Commerce System.",
    "keywords": ["dscmall", "ectouch", "ecshop", "ecmall", "wemall"],
    "homepage": "http://www.flyobd.com/",
    "license": "GPL",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "aliyuncs/oss-sdk-php": "^2.2",
        "dingo/api": "1.0.x@dev",
        "doctrine/dbal": "^2.5",
        "endroid/qr-code": "^2.5",
        "erusev/parsedown": "^1.6",
        "firebase/php-jwt": "^5.0",
        "laravel/lumen-framework": "5.4.*",
        "mobiledetect/mobiledetectlib": "^2.8",
        "overtrue/easy-sms": "^0.0.4",
        "overtrue/pinyin": "~3.0",
        "phpoffice/phpspreadsheet": "^1.1",
        "riverslei/payment": "^4.0",
        "sentry/sentry": "^1.6",
        "tymon/jwt-auth": "0.5.*",
        "vlucas/phpdotenv": "~2.2",
        "workerman/phpsocket.io": "^1.1"
    },
    "autoload": {
        "classmap": [
            "mobile/database"
        ],
        "psr-4": {
            "App\\": "mobile/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "mobile/tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}